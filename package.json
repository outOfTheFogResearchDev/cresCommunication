{
  "name": "crescommunication",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "test": "set NODE_ENV=test&&jest",
    "start": "set TYPE=local&&moku list&&moku list&&node server/index.js",
    "open": "start chrome /max --app=http://localhost:3330",
    "cpp-build": "node-gyp rebuild",
    "react-dev": "webpack -d --watch --optimize-minimize --optimize-occurrence-order --optimize-dedupe --config webpack.config.js",
    "deployment-build": "webpack -p --progress --optimize-minimize --optimize-occurrence-order --optimize-dedupe --config webpack.config.js",
    "server-build": "set TYPE=exe&&webpack -d --progress --optimize-minimize --optimize-occurrence-order --optimize-dedupe --config webpack.config.server.js",
    "setup": "npm i&&npm run deployment-build",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Max-Berkowitz/cresCommunication.git"
  },
  "author": "Max Berkowitz",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Max-Berkowitz/cresCommunication/issues"
  },
  "homepage": "https://github.com/Max-Berkowitz/cresCommunication#readme",
  "dependencies": {
    "axios": "^0.18.1",
    "bindings": "^1.3.1",
    "body-parser": "^1.18.3",
    "csv-parse": "^4.6.5",
    "csv-stringify": "^5.3.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "node-addon-api": "^1.6.1",
    "node-gyp": "^3.8.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "serialport": "^7.1.5",
    "styled-components": "^3.4.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.12.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
